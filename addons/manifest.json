{
  "manifest_version": 2,
  "name": "save all tab to github gist",
  "description": "save all tab to github gist \n",
  "version": "2024.1.15.4",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "save-all-tab-to-repository@example.com"
    }
  },
  "icons": {
    "48": "icons/48.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "menus",
    "notifications",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_title": "setting",
    "default_popup": "popup-browser-act.html"
  }
}